<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffff"
    tools:layout_editor_absoluteY="25dp">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="350dp"
        android:layout_height="395dp"
        android:src="@drawable/smartclinica"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="384dp"
        android:layout_height="0dp"
        android:layout_marginBottom="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/btnAjuda"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2">

        <ImageButton
            android:id="@+id/btnCadastrar"
            android:layout_width="112dp"
            android:layout_height="101dp"
            android:background="#ffff"
            android:src="@drawable/microphone64" />

        <ImageButton
            android:id="@+id/btnProntuario"
            android:layout_width="106dp"
            android:layout_height="101dp"
            android:background="#ffff"
            android:src="@drawable/contract64" />

        <ImageButton
            android:id="@+id/btnGPS"
            android:layout_width="103dp"
            android:layout_height="99dp"
            android:background="#ffff"
            android:src="@drawable/navigation64" />
    </LinearLayout>

    <ImageButton
        android:id="@+id/btnAjuda"
        android:layout_width="98dp"
        android:layout_height="0dp"
        android:layout_marginBottom="2dp"
        android:layout_marginLeft="121dp"
        android:layout_marginStart="121dp"
        android:background="#ffff"
        android:src="@drawable/info64"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        app:srcCompat="@drawable/info64" />
    tools:layout_editor_absoluteX="16dp"
    tools:layout_editor_absoluteY="480dp" />


</android.support.constraint.ConstraintLayout>